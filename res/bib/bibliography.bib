@article{art:konyagin00,
	author={S V Konyagin},
	title={On everywhere divergence of trigonometric Fourier series},
	journal={Sbornik: Mathematics},
	volume={191},
	number={1},
	pages={97},
	url={http://stacks.iop.org/1064-5616/191/i=1/a=A04},
	year={2000},
	abstract={The following theorem is established. Theorem . Let a function ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img1.gif] {$ \varphi\colon \lbrack 0,+\infty)\to[0,+\infty)$} and a sequence ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img2.gif] {$ \{\psi(m)\}$} satisfy the following condition: the function ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img3.gif] {$ \varphi(u)/u$} is non-decreasing on ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img4.gif] {$ (0,+\infty)$} , ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img5.gif] {$ \psi(m)\geqslant 1$} ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img6.gif] {$ (m=1,2,\dots)$} and ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img7.gif] {$ \varphi(m)\psi(m)=o(m\sqrt{\ln m}/\sqrt{\ln\ln m}\,)$} as ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img8.gif] {$ m\to\infty$} . Then there is a function ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img9.gif] {$ f\in L \lbrack -\pi,\pi \rbrack $} such that ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img10.gif] {$\displaystyle \int _{-\pi}^\pi\varphi(\vert f(x)\vert)\,dx<\infty$} and ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img11.gif] {$ \limsup_{m\to\infty}S_m(f,x)/\psi(m)=\infty$} for all ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img12.gif] {$ x\in \lbrack -\pi,\pi \rbrack $} here ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img13.gif] {$ S_m(f)$} is the ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img14.gif] {$ m$} -th partial sum of the trigonometric Fourier seriesof ##IMG## [http://ej.iop.org/images/1064-5616/191/1/A04/tex_sm_449_img15.gif] {$ f$} .}
}
